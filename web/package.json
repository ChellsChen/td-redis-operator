    "express-http-proxy": "^1.6.0",
    "express-session": "^1.17.1",
    "formidable": "1.2.1",
    "hox": "^1.1.1",
    "ioredis": "^4.17.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.17.11",
    "memoize-one": "5.0.0",
    "morgan": "1.9.1",
    "node-eunomia": "2.0.3",
    "node-graylog": "2.0.3",
    "prettier": "1",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
